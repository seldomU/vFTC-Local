{
  "name": "VRS",
  "version": "1.0.0",
  "description": "Downloadable Virtual Robot Simulator Platform",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-packager . --platform=win32 --arch=x64 --prune=true --out=release-builds --icon=logo.ico --overwrite",
    "package-mac": "electron-packager . --overwrite --platform=darwin --prune=true --arch=x64 --out=release-builds --icon=logo.icns",
    "build-installer": "cd release-builds; node build.js"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Virtual Robot Simulator",
    "FTC",
    "FIRST",
    "Virtual FTC",
    "VRS Platform"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^12.0.0",
    "electron-installer-dmg": "^3.0.0",
    "electron-packager": "^15.3.0"
  },
  "dependencies": {
    "blockly": "^4.20201217.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-winstaller": "^5.0.0",
    "net": "^1.0.2"
  }
}
